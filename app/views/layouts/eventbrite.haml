!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Eventbrite
    %meta{:content => "width=device-width,initial-scale=1", :name => "viewport"}/
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag 'phantom', media: 'all', 'data-turbolinks-track': 'reload'
    = stylesheet_link_tag 'parallelism', media: 'all', 'data-turbolinks-track': 'reload'


  %body{ style: "" }
    = yield

    = javascript_pack_tag 'eventbrite', 'data-turbolinks-track': 'reload'

